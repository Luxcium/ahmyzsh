{
    "editor.snippetSuggestions": "top",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "window.zoomLevel": -2,
    "git.enableSmartCommit": true,
    "javascript.validate.enable": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.enableBell": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.enableAppInsights": false,
    "rest-client.enableTelemetry": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "files.autoSave": "off",
    "workbench.activityBar.visible": true,
    "workbench.sideBar.location": "left",
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": true,
    "editor.minimap.enabled": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "git.autofetch": true,
    "workbench.colorTheme": "Popping and Locking",
    "editor.showUnused": true,
    "breadcrumbs.enabled": true,
    "git.confirmSync": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontLigatures": true,
    "workbench.fontAliasing": "default",
    "terminal.integrated.fontFamily": "DroidSansMono Nerd Font, Droid Sans Mono Nerd Font Complete, LigaFantasqueSansMono ,CQ Mono, Fira Code, monospace",
    "editor.fontFamily": "Fira Code",
    "editor.fontSize": 18,
    "editor.fontWeight": "bold",
    "terminal.integrated.fontSize": 16,
    "liveServer.settings.AdvanceCustomBrowserCmdLine": "/Applications/Chromium.app/ --incognito --headless --remote-debugging-port=9222",
    "editor.formatOnSave": true,
        "[javascript]": {
            "editor.formatOnSave": false,
        },
    "typescript.preferences.quoteStyle": "auto",
    "eslint.autoFixOnSave": true,
    "eslint.alwaysShowStatus": true,
    "eslint.run": "onSave",
    "material-icon-theme.opacity": 1,
    "coloredRegions.namedColors": {
        "Main": "rgba(79, 44, 115, 0.5)",
        "JavaScript": "rgba(115, 115, 44, 0.3)",
        "ImportsRequire": "rgba(44, 44, 115, 0.4)",
        "HEAD": "rgba(44, 44, 115, 0.4)",
        "Footer": "rgba(44, 44, 115, 0.4)",
        "BODY": "rgba(115, 115, 44, 0.3)",
        "Region": "rgba(115, 115, 44, 0.3)",
        "mainContent": "rgba(79, 44, 115, 0.5)"
    },
    "editor.tabSize": 2,
    "npm-intellisense.scanDevDependencies": true,
    "npm-intellisense.importES6": false,
    "editor.quickSuggestionsDelay": 10,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.tabCompletion": true,
    "javascript.nameSuggestions": false,
    "editor.wordBasedSuggestions": false,
    "material-icon-theme.folders.color": "#ef5350",
    "material-icon-theme.folders.associations": {
        "feature": "Git",
        "templates": "Class"

    },
    "material-icon-theme.files.associations": {
        "index.js": "Javascript-map"
    },
    "terminal.integrated.cursorStyle": "block",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "liveServer.settings.donotVerifyTags": true,

    // available through eslint plugin in vscode

}
