  "scripts": {
    "start": "npm run build ; node exports/server/MAIN.js",
    "debug": "npm run debug:build;  npm run compile:client:watch",
    "build": "concurrently \"npm:asset\" \"tcm src\" ;  concurrently \"npm:compile:client\" \"npm:compile:server\"  ",
    "build:watch": "concurrently \"npm:asset\" \"tcm src\"  ; concurrently \"npm:compile:client:watch\"  \"npm:compile:server:watch\"",
    "compile:server": "tsc  --build  tsconfig.json",
    "compile:client": "webpack --mode production",
    "compile:server:watch": "tsc --build --watch tsconfig.json",
    "compile:client:watch": "webpack --watch --mode development",
    "asset": " rm -rf ./exports/public; rm -rf ./exports/client/; rm -rf ./exports/server/; cp -vR ./src/assets ./exports/public",
    "nodemon:start": "killall node; nodemon exports/server/MAIN.js --config config/nodemon.json",
    "nodemon:inspect": "killall node; nodemon --inspect exports/server/MAIN.js  --config config/nodemon.json",
    "nodemon:inspect:brk": "killall node; nodemon --inspect-brk exports/server/MAIN.js  --config config/nodemon.json",
    "debug:build": "concurrently \"rm -rf ./exports/public\" \"rm -rf ./exports/client/\" \"tcm src\"; cp -vR ./src/assets ./exports/public"
  }
