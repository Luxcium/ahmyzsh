{
  "files.exclude": {
    "**/*.zwc": true,
    "powerlevel10k": true,
    "ohmyzsh": true,
    "powerline": true,
    "crt-profiles": true,
    "node_modules": true
  },
  "search.exclude": {
    "powerlevel10k": true,
    "ohmyzsh": true,
    "powerline": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "plugins": true,
    "archives": true,
  },
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  /*
        "#F15F",
        "#FB3F",
        "#4AEF",
        "#BD2F",
        */
  "editor.rulers": [
    {
      "column": 2,
      "color": "#BD28"
    },
    {
      "column": 80,
      "color": "#4AEF"
    },
    {
      "column": 81,
      "color": "#F15F"
    },
    {
      "column": 84,
      "color": "#BD24"
    },
    {
      "column": 88,
      "color": "#4AE6"
    },
    {
      "column": 93,
      "color": "#BD28"
    },
    {
      "column": 120,
      "color": "#F15F"
    },
    {
      "column": 121,
      "color": "#FA0E"
    },
    {
      "column": 122,
      "color": "#FD3E"
    },
    {
      "column": 123,
      "color": "#BD2F"
    },
    {
      "column": 124,
      "color": "#4AEF"
    },
    {
      "column": 125,
      "color": "#A2EF"
    },
    {
      "column": 128,
      "color": "#4AE3"
    },
    {
      "column": 131,
      "color": "#4AE6"
    },
    {
      "column": 134,
      "color": "#4AE9"
    },
    {
      "column": 138,
      "color": "#4AEC"
    }
  ],
  "better-comments.tags": [
    {
      // #region
      "tag": "#region",
      "color": "#555",
      "strikethrough": false,
      "backgroundColor": "#FF3D"
    },
    {
      // #endregion
      "tag": "#endregion",
      "color": "#222",
      "strikethrough": false,
      "backgroundColor": "#F15E"
    },
    {
      // !!
      "tag": "!!",
      "color": "#FF2D00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // ?
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // $
      "tag": "$",
      "color": "#f48e41",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // #
      "tag": "#",
      "color": "#4AE",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // IIFE
      "tag": "IIFE",
      "color": "#FF3D",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // private
      "tag": "private",
      "color": "#F15E",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // protected
      "tag": "protected",
      "color": "#FD3E",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // TEST_MODULE
      "tag": "TEST_MODULE",
      "color": "#6CBC",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // public
      "tag": "public",
      "color": "#FC5D", // "#FA0E",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // static
      "tag": "static",
      "color": "#F70E",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // get
      "tag": "get",
      "color": "#6BFC",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    // {
    //   // set
    //   "tag": "set",
    //   "color": "#85FE",
    //   "strikethrough": false,
    //   "backgroundColor": "transparent"
    // },
    {
      // %
      "tag": "%",
      "color": "#41e2f4",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // # -c-54442
      // †
      "tag": "†",
      "color": "#c54442",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // # -c-54442
      // ©
      "tag": "©",
      "color": "#c54442",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // +
      "tag": "+",
      "color": "#c5f442",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // async
      "tag": "async",
      "color": "#FB3F",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // http
      "tag": "http",
      "color": "#FB3F",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // &
      "tag": "&",
      "color": "#a041f4",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    // {
    //   //
    //   "tag": "",
    //   "color": "#6d6060",
    //   "strikethrough": false,
    //   "backgroundColor": "transparent"
    // },
    {
      // todo
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // *
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // ¬
      "tag": "¬",
      "color": "#ffa0a0",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    // {
    //       // async function
    //       "tag": "| async function |",
    //       "color": "#FB3F",
    //       "strikethrough": false,
    //       "backgroundColor": "transparent"
    // },
    {
      // -
      "tag": "-",
      "color": "#9BEA",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // ====
      "tag": "====",
      "color": "#9BEA",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      // setopt
      "tag": "setopt",
      "color": "#c5f442",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
  ],
  "window.title": "°Ah! MYZSH° ✨${remoteName}${appName}✨ ${rootPath} ${dirty} ${activeEditorMedium} ",
}
