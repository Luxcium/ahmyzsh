#!/bin/sh

#
myxuidhash="$(/home/luxcium/ahmyzsh/plugins/bin/randx 10)"
myxuiddate="$(date --utc +%C%y%m%d%H%M%S)$(date --utc +%N | cut -c -3)"
echo "${myxuidhash}UXHHZ${myxuiddate}"
# 62CEF7FEA120200717145814485
# 20200717145814485UXZ62CEF7FEA1
# 8370BCAEFEUXZ20200717150040721
# # +1 = 1
